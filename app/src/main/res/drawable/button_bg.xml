<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">


    <!-- Background color for the button -->
    <solid android:color="#FFFFFF" /> <!-- Replace with your desired color -->
    <solid android:color="#698C56"/>

    <!-- Border / Stroke -->
    <stroke
        android:width="2dp"
        android:color="#698C56" /> <!-- Set your border color here -->

    <!-- Rounded corners -->
    <corners android:radius="20dp" /> <!-- Adjust for the desired roundness -->

    <!-- Optional: Add padding inside the button for a better look -->
    <padding
        android:left="20dp"
        android:right="20dp"
        android:top="8dp"
        android:bottom="8dp" />

</shape>